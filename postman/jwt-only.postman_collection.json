{
  "info": {
    "name": "Ultra Simple S2S - JWT Only",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Healthz",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/api/healthz",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "healthz"
          ]
        }
      }
    },
    {
      "name": "Echo (com Bearer)",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{accessToken}}",
            "type": "text"
          },
          {
            "key": "Content-Type",
            "value": "application/json",
            "type": "text"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"msg\": \"ol√°\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/echo",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "echo"
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "https://localhost:8443"
    },
    {
      "key": "accessToken",
      "value": ""
    }
  ]
}